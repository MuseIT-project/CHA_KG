<rdf:Description rdf:about="http://www.semanticweb.org/museITchassets#CERTH_VR1_2">
	<CurrentLocation xmlns="http://www.semanticweb.org/museITchassets#" rdf:resource="http://www.semanticweb.org/museITchassets#LibraryofArcheologyandArtHistoryofRome"/>
